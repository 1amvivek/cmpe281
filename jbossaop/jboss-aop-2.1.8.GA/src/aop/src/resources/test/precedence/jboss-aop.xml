       <advice aspect="org.jboss.test.aop.precedence.LastAspect2" name="advice"/>
   </bind>
   
   <bind pointcut="execution(* org.jboss.test.aop.precedence.POJO->twoMethod()) OR call(* org.jboss.test.aop.precedence.POJO->fiveMethod())">
